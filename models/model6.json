{
  "state_vars": ["V", "I", "T"],
  "constants": {
    "dt": 0.002,
    "R": 1.0,
    "L": 0.1,
    "C": 0.01,
    "alpha": 0.5
  },
  "ranges": {
    "ep_V": [-0.02, 0.02],
    "ep_I": [-0.01, 0.01],
    "ep_T": [-0.05, 0.05]
  },
  "equations": {
    "V'": "V + dt * (- (V / R) - I / C) + ep_V",
    "I'": "I + dt * ((V - alpha * T) / L) + ep_I",
    "T'": "T + dt * (0.1 * I - 0.05 * T) + ep_T"
  },
  "constraints": [
    { "state": "V", "op": "ge", "const": 15.0 },
    { "state": "V", "op": "le", "const": -15.0 },
    { "state": "I", "op": "ge", "const": 5.0 },
    { "state": "I", "op": "le", "const": -5.0 },
    { "state": "T", "op": "ge", "const": 50.0 },
    { "state": "T", "op": "le", "const": -10.0 }
  ]
}

