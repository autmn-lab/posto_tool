{
  "state_vars": [
    "x1",
    "x2",
    "x3"
  ],
  "constants": {
    "dt": 0.1,
    "v_e": 1.0,
    "v_p": 1.5,
    "omega_e": 0.1,
    "omega_p": 0.2
  },
  "equations": {
    "x1'": "x1 + dt * (-v_e + v_p * cos(x3) + omega_e * x2)",
    "x2'": "x2 + dt * (v_p * sin(x3) - omega_e * x1)",
    "x3'": "x3 + dt * (omega_p - omega_e)"
  },
  "safety_constraints": [
    {
      "state": "x3",
      "op": "ge",
      "const": 3.14
    },
    {
      "state": "x3",
      "op": "le",
      "const": -3.14
    }
  ]
}
