{
  "state_vars": ["price", "trend", "vol", "shock"],
  "constants": {
    "dt": 0.01,
    "mu": 0.05,
    "lambda_const": 0.2,
    "theta": 0.1
  },
  "ranges": {
    "ep_price": [-0.1, 0.1],
    "ep_trend": [-0.05, 0.05],
    "ep_vol": [-0.02, 0.02],
    "ep_shock": [-1.0, 1.0]
  },
  "equations": {
    "price'": "price + dt * (mu * price + trend * vol) + ep_price",
    "trend'": "trend + dt * ((-1.0 * lambda_const) * trend + shock) + ep_trend",
    "vol'": "vol + dt * ((-1.0 * theta) * vol + 0.1 * sqrt(abs(shock))) + ep_vol",
    "shock'": "shock + dt * (-0.5 * shock) + ep_shock"
  },
  "safety_constraints": [
    { "state": "price", "op": "ge", "const": 7500.0 },
    { "state": "price", "op": "le", "const": 100.0 },
    { "state": "trend", "op": "ge", "const": 40.0 },
    { "state": "trend", "op": "le", "const": -40.0 },
    { "state": "vol", "op": "ge", "const": 4.0 },
    { "state": "vol", "op": "le", "const": 1.3 },
    { "state": "shock", "op": "ge", "const": 20.0 },
    { "state": "shock", "op": "le", "const": -20.0 }
  ]
}

